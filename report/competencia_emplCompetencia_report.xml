<odoo>
<record id="competencia.emplCompetencia_report" model="ir.actions.report">
<field name="name">Informe competencia</field>
<field name="model">jugador.competencia</field>
<field name="report_type">qweb-pdf</field>
<field name="report_name">jugador.competencia_report_view</field>
<field name="report_file">jugador.competencia_report</field>
<field name="binding_model_id" ref="model_jugador_competencia"/>
<field name="binding_type">report</field>
</record>
<template id="competencia_report_view">
<t t-call="web.html_container">
<t t-foreach="docs" t-as="competencia">
<t t-call="web.external_layout">
<div class="page">
<h2>INFORME</h2>
<h3 t-field="competencia.nombreCompetencia"/>
<h4>Descripcion </h4>
<h4 t-field="competencia.descripcionCompetencia"/>
<table class="table table-sm o_main_table">
<thead>
<tr>
<th>DNI</th>
<th>Nombre</th>
<th>Fecha de nacimiento</th>
<th>Edad</th>
<th>Direcci√≥n</th>
<th>Telefono</th>
</tr>
</thead>
<tbody>
<t t-foreach="competencia.empleado_id" t-as="empleado">
<tr>
<td>
<span t-field="empleado.dniEmpleado"/>
</td>
<td>
<span t-field="empleado.nombreEmpleado"/>
</td>
<td>
<span t-field="empleado.fechaNacimiento"/>
</td>
<td>
<span t-field="empleado.edad"/>
</td>
<td>
<span t-field="empleado.direccionEmpleado"/>
</td>
<td>
<span t-field="empleado.telefonoEmpleado"/>
</td>
</tr>
</t>
</tbody>
</table>
</div>
</t>
</t>
</t>
</template>
</odoo>